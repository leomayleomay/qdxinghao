<%= sidebar %>

<table border="0" cellpadding="0" cellspacing="0" id="data">
  <tr>
    <th>标题</th>
  </tr>

<% @messages.each do |message| %>
  <tr>
    <td><%=h message.title %></td>
    <td><%= link_to '详情', message %></td>
  </tr>
<% end %>
  <tr>
    <th colspan='3' class="pagination"> <%= will_paginate @messages%></th>
  </tr>
</table>
